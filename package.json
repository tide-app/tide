{
  "name": "tide",
  "version": "1.0.0",
  "description": "Explore a world of sounds",
  "keywords": [
    "react",
    "starter"
  ],
  "homepage": "https://tide-app.vercel.app",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:style && react-scripts build",
    "build:icons": "pwa-asset-generator ./public/icons/light-icon.svg public/icons --padding 0px --background '#FBDC57' --index ./public/index.html --path '%PUBLIC_URL%' --manifest public/manifest.json && pwa-asset-generator ./public/icons/dark-icon.svg public/icons --padding 0px --background 'black' --dark-mode --splash-only --index ./public/index.html --path '%PUBLIC_URL%' && prettier 'public/index.html' --write",
    "build:style": "tailwindcss build src/styles.css -o src/build.css",
    "e2e": "cypress run",
    "eject": "react-scripts eject",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "start": "npm run build:style && react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "last 2 chrome versions"
  ],
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "extends": [
      "bliss",
      "plugin:import/typescript",
      "plugin:react/recommended",
      "plugin:cypress/recommended"
    ],
    "overrides": [
      {
        "extends": [
          "plugin:@typescript-eslint/recommended"
        ],
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ]
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/extensions": "off",
          "import/no-anonymous-default-export": "off"
        }
      }
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "babelOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      },
      "requireConfigFile": false
    },
    "rules": {
      "react/prop-types": "off"
    }
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@colormap/core": "0.2.0",
    "@colormap/presets": "0.2.0",
    "@headlessui/react": "^0.2.0",
    "@ionic/react": "^5.5.2",
    "@palette.dev/browser": "^1.5.3",
    "@palette.dev/webpack-plugin": "^1.1.4",
    "@sentry/react": "^7.38.0",
    "@sentry/tracing": "^6.14.1",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@tsconfig/node12": "^1.0.7",
    "@types/cypress": "^1.1.3",
    "@types/lodash": "^4.14.166",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-modal": "^3.10.6",
    "@types/react-router-dom": "^5.1.6",
    "autoprefixer": "^10.4.13",
    "bundlewatch": "^0.3.1",
    "cypress": "6.2.0",
    "dompurify": "^2.2.6",
    "eslint": "^8.35.0",
    "eslint-config-bliss": "^6.0.5",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "freesound-client": "^0.5.0",
    "husky": "^4.3.6",
    "ionicons": "^5.2.3",
    "levenary": "^1.1.1",
    "lint-staged": "^10.5.3",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "postcss": "8.x.x",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "pwa-asset-generator": "^4.0.2",
    "react": "18.x.x",
    "react-dom": "18.x.x",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "react-wavesurfer.js": "^0.0.8",
    "tailwindcss": "^2.2.19",
    "tinykeys": "^1.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "use-debounce": "^5.2.0",
    "wavesurfer.js": "^6.4.0",
    "web-audio-test-api": "^0.5.2"
  },
  "bundlewatch": [
    {
      "path": "./build/static/js/main.*.chunk.js",
      "maxSize": "10 kB"
    }
  ],
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "webpack": "^5.76.0"
  },
  "overrides": {
    "react-refresh": "0.14.0"
  }
}
