{
  "name": "freesound-player",
  "version": "1.0.0",
  "description": "freesound player",
  "license": "MIT",
  "homepage": "https://freesound-player.vercel.app",
  "keywords": [
    "react",
    "starter"
  ],
  "dependencies": {
    "@colormap/core": "0.2.0",
    "@colormap/presets": "0.2.0",
    "@ionic/react": "^5.4.3",
    "@types/lodash": "^4.14.165",
    "autoprefixer": "^10.0.1",
    "freesound-client": "^0.2.2",
    "ionicons": "^5.2.3",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "postcss-cli": "^8.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "^4.0.0",
    "react-wavesurfer.js": "^0.0.2",
    "tailwindcss": "^1.9.6",
    "tinykeys": "^1.1.1",
    "use-debounce": "^5.0.3",
    "wavesurfer.js": "4.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@tsconfig/node12": "^1.0.7",
    "@types/cypress": "^1.1.3",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "bundlesize": "^0.18.0",
    "eslint-config-bliss": "^5.0.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "levenary": "^1.1.1",
    "lint-staged": "^10.5.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:style": "tailwind build src/styles.css -o src/tailwind.css",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "e2e": "cypress run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "bliss",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "bundlesize": [
    {
      "path": "./build/static/js/main.*.chunk.js",
      "maxSize": "10 kB"
    }
  ]
}
