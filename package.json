{
  "name": "freesound-player",
  "version": "1.0.0",
  "description": "freesound player",
  "license": "MIT",
  "homepage": "https://freesound-player.netlify.app",
  "keywords": [
    "react",
    "starter"
  ],
  "dependencies": {
    "@colormap/core": "0.2.0",
    "@colormap/presets": "0.2.0",
    "autoprefixer": "^10.0.1",
    "freesound-client": "^0.2.2",
    "localforage": "^1.9.0",
    "lodash": "4.17.20",
    "postcss-cli": "^8.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "react-wavesurfer.js": "^0.0.2",
    "tailwindcss": "^1.8.11",
    "tinykeys": "^1.1.0",
    "wavesurfer.js": "4.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@tsconfig/node12": "^1.0.7",
    "@types/cypress": "^1.1.3",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "cypress": "^5.3.0",
    "eslint-config-bliss": "^5.0.0",
    "eslint-plugin-react": "^7.21.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint . --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:style": "tailwind build src/styles.css -o src/tailwind.css",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "e2e": "cypress run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "renovate": {
    "extends": [
      "bliss"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "bliss",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": "off"
    }
  }
}
